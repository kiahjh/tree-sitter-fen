{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "fen",
  "rules": {
    "source_file": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "metadata"
        },
        {
          "type": "SYMBOL",
          "name": "divider"
        },
        {
          "type": "SYMBOL",
          "name": "route_io"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "divider"
                },
                {
                  "type": "SYMBOL",
                  "name": "helper_definitions"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "_whitespace": {
      "type": "PATTERN",
      "value": "\\s+"
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "//"
          },
          {
            "type": "PATTERN",
            "value": ".*"
          }
        ]
      }
    },
    "metadata": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "name_metadata_entry"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "description_metadata_entry"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "authed_metadata_entry"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "name_metadata_entry": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "name_metadata_keyword"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "SYMBOL",
          "name": "string_literal"
        }
      ]
    },
    "name_metadata_keyword": {
      "type": "TOKEN",
      "content": {
        "type": "STRING",
        "value": "name"
      }
    },
    "description_metadata_entry": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "description_metadata_keyword"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "SYMBOL",
          "name": "string_literal"
        }
      ]
    },
    "description_metadata_keyword": {
      "type": "TOKEN",
      "content": {
        "type": "STRING",
        "value": "description"
      }
    },
    "authed_metadata_entry": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "authed_metadata_keyword"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "SYMBOL",
          "name": "boolean_literal"
        }
      ]
    },
    "authed_metadata_keyword": {
      "type": "TOKEN",
      "content": {
        "type": "STRING",
        "value": "authed"
      }
    },
    "route_io": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "io_definition"
      }
    },
    "io_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "io_label"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "struct_definition_block"
            },
            {
              "type": "SYMBOL",
              "name": "enum_definition_block"
            }
          ]
        }
      ]
    },
    "io_label": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "helper_definitions": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "helper_definition"
        }
      ]
    },
    "helper_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "struct_definition_block"
            },
            {
              "type": "SYMBOL",
              "name": "enum_definition_block"
            }
          ]
        }
      ]
    },
    "divider": {
      "type": "STRING",
      "value": "---"
    },
    "string_literal": {
      "type": "PATTERN",
      "value": "\"[^\"]*\""
    },
    "boolean_literal": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "true"
        },
        {
          "type": "STRING",
          "value": "false"
        }
      ]
    },
    "struct_definition_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "struct_field"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "struct_field": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "SYMBOL",
          "name": "type"
        }
      ]
    },
    "enum_definition_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "enum_variant"
          }
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "enum_variant": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "("
                },
                {
                  "type": "SYMBOL",
                  "name": "type"
                },
                {
                  "type": "STRING",
                  "value": ")"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_]*"
    },
    "type": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "int_type"
        },
        {
          "type": "SYMBOL",
          "name": "float_type"
        },
        {
          "type": "SYMBOL",
          "name": "boolean_type"
        },
        {
          "type": "SYMBOL",
          "name": "string_type"
        },
        {
          "type": "SYMBOL",
          "name": "uuid_type"
        },
        {
          "type": "SYMBOL",
          "name": "date_type"
        },
        {
          "type": "SYMBOL",
          "name": "array_type"
        },
        {
          "type": "SYMBOL",
          "name": "optional_type"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "int_type": {
      "type": "STRING",
      "value": "Int"
    },
    "float_type": {
      "type": "STRING",
      "value": "Float"
    },
    "string_type": {
      "type": "STRING",
      "value": "String"
    },
    "boolean_type": {
      "type": "STRING",
      "value": "Boolean"
    },
    "uuid_type": {
      "type": "STRING",
      "value": "UUID"
    },
    "date_type": {
      "type": "STRING",
      "value": "Date"
    },
    "array_type": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "SYMBOL",
          "name": "type"
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "optional_type": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "type"
        },
        {
          "type": "STRING",
          "value": "?"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "SYMBOL",
      "name": "comment"
    },
    {
      "type": "SYMBOL",
      "name": "_whitespace"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}