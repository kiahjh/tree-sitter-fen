{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "fen",
      "camelcase": "Fen",
      "title": "Fen",
      "scope": "source.fen",
      "file-types": ["fen"],
      "injection-regex": "^fen$",
      "class-name": "TreeSitterFen"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "A simple SDL for generating typesafe and cross-language code.",
    "authors": [
      {
        "name": "Miciah Henderson",
        "email": "miciahjohnhenderson@gmail.com",
        "url": "https://kiahjh.com/"
      }
    ],
    "links": {
      "repository": "https://github.com/kiahjh/fen"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
